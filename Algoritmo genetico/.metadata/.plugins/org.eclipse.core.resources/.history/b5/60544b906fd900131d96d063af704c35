#include <stdio.h>
#include <stdlib.h>

#define GEN_SIZE 10
#define POP_SIZE 100

typedef struct{
	char genotype[GEN_SIZE];
	float fitness;
}individual;

individual pop[POP_SIZE];

void initialize_population(individual * pop){

	int i,j;
	for(i=0; i< POP_SIZE;i++){
		for(j=0;j<GEN_SIZE){
			pop[i].genotype[j] = rand() % 2;
		}
	}


}



int main(int argc, char **argv){


	printf("hello!");


}
